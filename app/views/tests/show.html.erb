<h2>Test ID:<%= @test.id %></h2>
<p><%=@test.title %></p>
<p>Level: <%= @test.level %></p>

<div>
  <table border="1">
    <tr>
      <th>Question ID:</th>
      <th>Body: </th>
    </tr>
    <% @test.questions.each do |q|%>
      <tr>
        <td>
          <%= q.id %>
        </td>
        <td>
          <%= link_to q.body, q %>
        </td>
      </tr>
    <% end %>
  </table>
</div>
<%= link_to 'Back', tests_path%>
<div>
  <%= button_to 'Edit Test', edit_test_path(@test)%>
</div>